<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.weather">
    <uses-permission-sdk-23 android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@drawable/weather_icon"
        android:label="天气app"
        android:roundIcon="@drawable/weather_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Weather">
        <activity android:name=".baseClass.HttpIfAddCityExistActivity" />
        <activity android:name=".ManageCity.CityEditActivity" />
        <activity android:name=".ManageCity.CityAddActivity" />
        <activity android:name=".ManageCity.CityManageActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>