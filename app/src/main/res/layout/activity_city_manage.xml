<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/manage_title"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="#fff">
        <ImageView
            android:id="@+id/manage_iv_back"
            android:layout_width="33dp"
            android:layout_height="33dp"
            android:layout_marginTop="6dp"
            android:layout_alignTop="@id/manage_iv_delete"
            android:layout_marginLeft="10dp"
            android:src="@drawable/back"/>
        <ImageView
            android:id="@+id/manage_iv_delete"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_alignParentRight="true"
            android:layout_marginTop="5dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/delete"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="城市管理"
            android:layout_centerInParent="true"
            android:textSize="22dp"
            android:textStyle="bold"/>

    </RelativeLayout>
    <ImageView
        android:id="@+id/manage_line1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#0E0101"
        android:layout_below="@id/manage_title"/>
    <ListView
        android:id="@+id/manage_listView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/manage_line1"
        android:divider="@null"/>
    <ImageView
        android:id="@+id/manage_add"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:src="@drawable/add"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_margin="25dp"/>




</RelativeLayout>